---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Tim Benniks</title>
  <link href="http://timbenniks.github.com/atom.xml" rel="self"/>
  <link href="http://timbenniks.github.com/"/>
  <updated>{{site.time | date_to_xmlschema }}</updated>
  <id>http://timbenniks.github.com</id>
  <author>
    <name>Tim Benniks</name>
    <email></email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://timbenniks.github.com"/>
    <updated>{{post.date | date_to_xmlschema }}</updated>
    <id>http://timbenniks.github.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>